{{#each shoppingCart.items as |item|}}
   <br>
  <div class="cart">
  		<div class="panel panel-default">
  		  <div class="panel-heading">
  		    <h3 class="panel-title title">{{item.name}}</h3>
  		  </div>
  		  <div class="panel-body">
  		    <div class="cartDetails">
  		    	<div class="col-md-6">
  		    		{{item.content}}
  		    	</div>
  		    	<div class="col-md-4">
  		    		<img src="{{item.image}}" alt="">
  		    	</div>
  		    </div>
  		  </div>
  		</div>
  	</div>
{{/each}}